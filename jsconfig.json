{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",
      "baseUrl": "src",
      "paths": {
        "@/*": ["src/*"],
        "assets/*": ["assets/*"],
        "@components/*": ["components/*"],
        "@pages/*": ["pages/*"],
        "@utils/*": ["utils/*"]
      },
      "jsx": "react-jsx",
      "allowSyntheticDefaultImports": true
    },
    "include": ["src/**/*"], 
    "exclude": ["node_modules", "dist"]
  }